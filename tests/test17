--TEST--
Tests assignment parser.

--INPUT--
names = ['Raoul', 'Niels', 'Kyron'];
names[1] = 'Sander';
names[2] = [0, 1, 2, 3];
names[2][3] += 97;

user = {'name': 'Raoul', 'friends': ['Niels', 'Kyron', 'Sander', 10]};
user['friends'][0] = 'Poes';
user['friends'][1] = 'Hond';
user['friends'][2] = 'Robin';
user['friends'][3] += 5;

return [
    names,
    user
];

--RESULT--
[[Raoul, Sander, [0, 1, 2, 100]], {name=Raoul, friends=[Poes, Hond, Robin, 15]}]