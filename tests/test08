--TEST--
Tests function and anonymous function declaration.

--INPUT--
fn a() {
    return;
}

fn b(x) {
    return x + 2;
}

fn c(x, y) {
    return x + y;
}

aa = fn() {
    return;
}

bb = fn(x) {
    return x + 2;
}

cc = fn(x, y) {
    return x + y;
}

return [a(), b(5), c(4, 3), aa(), bb(10), cc(5, 10)];

--RESULT--
[null, 7, 7, null, 12, 15]