--TEST--
Tests typehinting

--INPUT--
using('std.Writeln');

fn sum(num a, num b)
{
    return a + b;
}

class Greeter()
{
    fn Greet(str name)
    {
        Writeln("Hello " + name + "!");
    }
}

fn GreetProxy(str who, Greeter g)
{
    g.Greet(who);
}

Writeln(sum(1, 2));

b = Greeter();

GreetProxy("Raoul", b);

--OUTPUT--
3
Hello Raoul!