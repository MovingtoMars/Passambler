--TEST--
Tests while statement.

--INPUT--
a = 5
b = 0

while a < 10 {
    a += 1
    b += 1
}

x = 0
while x < 15 {
    x += 1

    if x == 5 {
        skip
    } elif x == 6 {
        x += 2
    } elif x == 9 {
        stop
    }
}

y = 0
while {
    y += 1
    if y == 9 {
        skip
    } elif y == 16 {
        stop
    }
    y += 1
}

return [a, b, x, y]

--RESULT--
[10, 5, 9, 16]