--TEST--
Tests classes.

--INPUT--
using('std.Writeln');

class Greeter(name, manner="Hello")
{
    fn Greet()
    {
        Writeln(self.manner + " " + self.name);
    }
}

g = Greeter("Raoul");
g.Greet();

a = Greeter(manner="Goodmorning", name="Passambler");
a.Greet();

g.Greet();

--OUTPUT--
Hello Raoul
Goodmorning Passambler
Hello Raoul