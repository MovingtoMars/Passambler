--TEST--
Tests classes.

--INPUT--
using('std.Writeln');

class Hello()
{
    fn Hello()
    {
        Writeln("hello");
    }

    fn Greet(who="User")
    {
        Writeln("greetings " + who);
    }
}

h = Hello.New();
h.Greet();
h.Greet("Raoul");

class Math()
{
    fn sum(a, b) { return a + b; }
    fn mul(a, b) { return a * b; }
}

class TestMath(n1, n2) : Math
{
    fn TestMath()
    {
        Writeln(this.sum(this.n1, this.n2));
        Writeln(this.mul(this.n2, this.n1));
    }
}

tm = TestMath.New(n2=3, n1=4);

--OUTPUT--
hello
greetings User
greetings Raoul
7
12