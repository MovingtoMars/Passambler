import 'std'
import 'math'

x = math.Rand(1, 100)

while {
    y = 0

    try {
        y = std.ToNum(readln())
    } catch {
        skip
    }

    if y > x {
        writeln('Lower')
    } elif y < x {
        writeln('Higher')
    } elif y == x {
        writeln('Correct!')

        stop
    }
}